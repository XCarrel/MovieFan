@model List<Movie>
@{
    ViewData["Title"] = "index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="container">
        <h1>Liste des films</h1>
        @foreach (Movie movie in Model)
        {
            <div class="row border p-3">
                @movie.Title
            </div>
        }
    </div>

