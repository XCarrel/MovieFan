@model MovieFan.Models.Users

@{
    ViewData["Title"] = "ManageMovielist";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Films commentés par @Model.FullName</h1>
@await Html.PartialAsync("_MyMovies", Model)
<a asp-action="AddMovieReview" asp-route-id="@Model.Id" class="btn btn-sm btn-primary ml-5">Ajouter une revue de film</a>
<script src="\js\managemovies.min.js"></script>